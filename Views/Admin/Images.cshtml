@model IEnumerable<finalmawjoud_nlh.Models.Images>

@{
    ViewBag.Title = "Images";
}

<h2>Images</h2>

<p>
    @Html.ActionLink("Create New", "CreateImage")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SlideImageName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SlideImage)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MenuImageName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MenuImage)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DiscountImageName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DiscountImage)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.otherImageName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OtherImage)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SlideImageName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SlideImage)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MenuImageName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MenuImage)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DiscountImageName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DiscountImage)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.otherImageName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OtherImage)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ImageId }) |
            @Html.ActionLink("Details", "Details", new { id=item.ImageId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ImageId })
        </td>
    </tr>
}

</table>
